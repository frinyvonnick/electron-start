{
  "name": "electron-start",
  "productName": "Electron-start",
  "version": "0.0.0",
  "description": "Electron bootstrap",
  "license": "MIT",
  "repository": "https://github.com/frinyvonnick/electron-start",
  "homepage": "https://github.com/frinyvonnick/electron-start",
  "bugs": "frin.yvonnick@gmail.com",
  "engines": {},
  "contributors": [],
  "author": {
    "name": "FRIN Yvonnick",
    "email": "frin.yvonnick@gmail.com",
    "url": "https://github.com/frinyvonnick"
  },
  "_moduleAliases": {
    "src": "./src",
    "assets": "./src/assets",
    "windows": "./src/windows",
    "main-process": "./src/main-process",
    "renderer-process": "./src/renderer-process"
  },
  "build": {
    "publish": {
      "GithubOptions": {

      }
    }
  },
  "scripts": {
    "start": "electron src",
    "dev": "cross-env NODE_ENV=development nodemon -e js --watch src/main-process --watch src/index.js --exec 'npm start'",
    "lint": "standard",
    "pack": "build --dir -mwl --x64",
    "dist": "build -mwl --x64"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "electron-debug": "^1.1.0",
    "electron-updater": "^1.11.2",
    "electron-window": "^0.8.1",
    "glob": "^7.1.1",
    "module-alias": "^2.0.0"
  },
  "devDependencies": {
    "cross-env": "^4.0.0",
    "devtron": "^1.1.0",
    "electron": "^1.4.0",
    "electron-builder": "^16.6.1",
    "nodemon": "^1.11.0",
    "standard": "^8.2.0"
  }
}
